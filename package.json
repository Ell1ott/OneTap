{
  "name": "OneTap",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "editor:dev": "vite --config ./editor-web/vite.config.ts -w build",
    "editor:build": "vite --config ./editor-web/vite.config.ts build && npm run editor:post-build",
    "editor:post-build": "node ./node_modules/@10play/tentap-editor/scripts/buildEditor.js ./editor-web/build/index.html",
    "reverse-android": "adb reverse tcp:3000 tcp:3000"
  },
  "dependencies": {
    "@10play/react-native-web-webview": "^0.0.3",
    "@10play/tentap-editor": "^0.7.0",
    "@deepgram/sdk": "^4.2.0",
    "@expo-google-fonts/inter": "^0.3.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@siteed/expo-audio-studio": "^2.9.0",
    "@tiptap/react": "^2.12.0",
    "cross-fetch": "^4.1.0",
    "deepgram": "^1.0.3",
    "dotenv": "^16.5.0",
    "expo": "^53.0.9",
    "expo-audio": "^0.4.5",
    "expo-av": "^15.1.4",
    "expo-constants": "^17.1.6",
    "expo-crypto": "^14.1.4",
    "expo-dev-client": "^5.1.8",
    "expo-file-system": "^18.1.10",
    "expo-font": "^13.3.1",
    "expo-haptics": "^14.1.4",
    "expo-image": "^2.1.7",
    "expo-linking": "^7.1.5",
    "expo-router": "^5.0.7",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "^0.30.8",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.511.0",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.79.2",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "^15.12.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "^13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/metro-runtime": "^5.0.4",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.25.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-singlefile": "^2.2.0"
  },
  "main": "expo-router/entry",
  "private": true
}
